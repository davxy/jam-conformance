{
  "polkajam": {
    "repo": "none",
    "file": "polkajam-fuzz",
    "cmd": "polkajam-fuzz",
    "args": "--source remote --step-period 0 --target-sock {TARGET_SOCK}",
    "env": "RUST_LOG=off"
  },
  "polkajam_int": {
    "repo": "none",
    "file": "polkajam-fuzz",
    "cmd": "polkajam-fuzz",
    "args": "--source remote --step-period 0 --target-sock {TARGET_SOCK} --pvm-interpreter-backend",
    "env": "RUST_LOG=off"
  },
  "vinwolf": {
    "repo": "bloppan/conformance_testing",
    "clone": 1,
    "file": "linux/tiny/x86_64/vinwolf-target",
    "cmd": "linux/tiny/x86_64/vinwolf-target --fuzz {TARGET_SOCK}"
  },
  "jamzig": {
    "repo": "jamzig/conformance-releases",
    "clone": 1,
    "file": {
      "linux": "tiny/linux/x86_64/jam_conformance_target",
      "macos": "tiny/macos/aarch64/jam_conformance_target"
    },
    "cmd": {
      "linux": "tiny/linux/x86_64/jam_conformance_target",
      "macos": "tiny/macos/aarch64/jam_conformance_target"
    },
    "args": "--socket={TARGET_SOCK}"
  },
  "pyjamaz": {
    "image": "jamdottech/pyjamaz:latest",
    "cmd": "fuzzer target --db-path=/tmp/pyjamaz_fuzzer_db --socket-path={TARGET_SOCK}"
  },
  "jampy": {
    "repo": "dakk/jampy-releases",
    "clone": 1,
    "file": "dist/jampy-target-0.7.1_x86-64.zip",
    "cmd": "jampy-target-0.7.1_x86-64/jampy-target-0.7.1_x86-64 --socket-file {TARGET_SOCK}"
  },
  "jamduna": {
    "repo": "jam-duna/jamtestnet",
    "file": {
      "linux": "duna_target_linux",
      "macos": "duna_target_mac"
    },
    "cmd": {
      "linux": "duna_target_linux",
      "macos": "duna_target_mac"
    },
    "args": "--socket {TARGET_SOCK}"
  },
  "jamixir": {
    "repo": "jamixir/jamixir-releases",
    "file": {
      "linux": "jamixir_linux-x86-64_0.7.1_tiny.tar.gz",
      "macos": "jamixir_macos-arm64_0.7.1_tiny.tar.gz"
    },
    "cmd": "jamixir fuzzer --log error --socket-path {TARGET_SOCK}"
  },
  "javajam": {
    "image": "ghcr.io/methodfive/javajam:latest-amd64",
    "cmd": "-Dskip.warmup=true fuzz {TARGET_SOCK}"
  },
  "jamzilla": {
    "repo": "ascrivener/jamzilla-conformance-releases",
    "file": {
      "linux": "fuzzserver-tiny-amd64-linux",
      "macos": "fuzzserver-tiny-arm64-darwin"
    },
    "cmd": {
      "linux": "fuzzserver-tiny-amd64-linux",
      "macos": "fuzzserver-tiny-arm64-darwin"
    },
    "args": "-socket {TARGET_SOCK}"
  },
  "spacejam": {
    "repo": "spacejamapp/specjam",
    "file": {
      "linux": "spacejam-0.7.1-linux-amd64.tar.gz",
      "macos": "spacejam-0.7.1-macos-arm64.tar.gz"
    },
    "cmd": "spacejam fuzz target {TARGET_SOCK}"
  },
  "tsjam": {
    "repo": "vekexasia/tsjam-releases",
    "file": "tsjam-fuzzer-target.tgz",
    "cmd": "tsjam-fuzzer-target/jam-fuzzer-target --socket {TARGET_SOCK}",
    "env": "JAM_CONSTANTS=tiny"
  },
  "boka": {
    "image": "acala/boka:latest",
    "cmd": "fuzz target --socket-path {TARGET_SOCK}"
  },
  "turbojam": {
    "image": "r2rationality/turbojam-fuzz:latest",
    "cmd": "fuzzer-api {TARGET_SOCK}"
  },
  "graymatter": {
    "image": "ghcr.io/jambrains/graymatter/gm:conformance-fuzzer-latest",
    "cmd": "fuzz-m1-target --stay-open --listen {TARGET_SOCK}"
  },
  "fastroll": {
    "repo": "fastroll-jam/fastroll-releases",
    "file": {
      "linux": "fastroll-linux-x86_64-tiny",
      "macos": "fastroll-macos-aarch64-tiny"
    },
    "cmd": {
      "linux": "fastroll-linux-x86_64-tiny",
      "macos": "fastroll-macos-aarch64-tiny"
    },
    "args": "fuzz --socket {TARGET_SOCK}"
  },
  "gossamer": {
    "repo": "chainsafe/gossamer-jam-releases",
    "file": {
      "linux": "gossamer-jam-tiny-linux-amd64",
      "macos": "gossamer-jam-tiny-macos-arm64"
    },
    "cmd": {
      "linux": "gossamer-jam-tiny-linux-amd64",
      "macos": "gossamer-jam-tiny-macos-arm64"
    },
    "args": "target --socket {TARGET_SOCK}"
  },
  "tessera": {
    "repo": "Chainscore/tessera-releases",
    "file": {
      "linux": "tessera-node-Linux-x64.tar.gz",
      "macos": "tessera-node-Darwin-arm64.tar.gz"
    },
    "cmd": "tessera-node --fuzzer --socket {TARGET_SOCK}"
  },
  "typeberry": {
    "image": "ghcr.io/fluffylabs/typeberry:latest",
    "cmd": "--version=1 fuzz-target {TARGET_SOCK}",
    "env": "JAM_LOG=log"
  },
    "strawberry": {
    "repo": "eigerco/strawberry",
    "file": "strawberry-linux-x86_64",
    "cmd": "strawberry-linux-x86_64",
    "args": "--socket {TARGET_SOCK}"
  }
}
